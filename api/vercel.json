{
  "version": 2,
  "builds": [
    {
      "src": "./src/api.gleam",
      "use": "@vercel/rust"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404.html"
    }
  ],
  "functions": {
    "api/index": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
